{
  "opts": {
    "encoding": "utf8",
    "destination": "./docs/",
    "recurse": true
  },
  "source": {
    "include": ["src/providers.js", "./README.md"],
    "includePattern": ".+\\.js(doc)?$"
  },
  "templates": {
    "default": {
      "outputSourceFiles": false,
      "staticFiles": {
        "include:": ["./logo.png"]
      }
    }
  }
}